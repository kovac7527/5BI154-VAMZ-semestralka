<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!--When Button is not enabled -->
    <item android:state_enabled="false" android:color="#b6b7b5" />
    <!--When Button is in pressed state -->
    <item android:state_pressed="true" android:color="#7fd4a9" />
    <!--When Button is in selected state -->
    <item android:state_selected="true" android:color="#17e37c" />
    <!--Default Background Color -->
    <item android:state_selected="false" android:color="#9fbfb5" />


</selector>